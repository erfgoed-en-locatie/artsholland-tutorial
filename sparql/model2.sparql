PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?predicate
  ?p_type ?p_label 
  ?p_domain ?p_range
WHERE {	
	OPTIONAL {
		?predicate rdf:type ?p_type ;
			rdfs:label ?p_label ;
			rdfs:domain ?p_domain ;
			rdfs:range ?p_range .	
	}
	?subject a <??class> ;
		?predicate ?object .
} LIMIT 10